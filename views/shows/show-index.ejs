<% include ../partials/boilerplate %>
<% include nav %>

<div class='showindex'>
  <h2>Top TV Shows</h2>

  <div class='show-container'>
    <% for (let show of data) { %>
      <div class='show'>
        <h3><%= show.title %></h3>
        <a class='linkto' href='/shows/<%= show.id %>'>Info</a>
      </div>
    <% } %>
  </div>
</div>

<% include ../partials/end %>
