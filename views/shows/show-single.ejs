<% include ../partials/boilerplate %>
<% include ../partials/nav %>

<div class='showindex'>
<h2><%= show.name %></h2>

  <div class='show-container'>
    <% for (let show of data) { %>
      <div class='show'>
        <img src=<%= show.image_thumbnail_path %>>
        <p><%= show.country %></p>
        <p><%= show.network %></p>
        <p><%= show.status %></p>
      </div>
    <% } %>
  </div>
</div>

<% include ../partials/end %>
